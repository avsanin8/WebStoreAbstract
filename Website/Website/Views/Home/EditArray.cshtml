@model List<Website.Models.Products>

@{
    ViewBag.Title = "EditArray";
}

<h2>EditArray</h2>

@using (Html.BeginForm())
{
    for (int i = 0; i < Model.Count; i++)
    {
        <h4>Product numb# @(i+1)</h4>
        @: Name: @Html.EditorFor(m => m[i].Name)
        @: Manufacture: @Html.EditorFor(m => m[i].Manufacture) 
        @: Price: @Html.EditorFor(m => m[i].Price)
    }
    <input type="submit" value="EditArray"/>
}
